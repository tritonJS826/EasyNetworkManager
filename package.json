    "node-nmap": "^4.0.0",
    "node-sass": "^4.14.1",
    "ping": "^0.2.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-bash": "^1.6.0",
    "react-component-console": "^1.2.2",
    "react-dom": "^16.8.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^3.4.3",
    "redux": "^4.0.5",
